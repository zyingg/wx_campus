.img{
  display: flex;
  justify-content: center;
}
.img image{
  width: 300rpx;
  height: 300rpx;
  /* border: 1px solid #000000; */
  border-radius: 50%;
  display: flex;
  justify-content: center;
  margin-top: 80rpx;
}
.info{
  display: column;
  text-align: center;
  width: 100%;
  height: 150rpx;
  margin-top: 70rpx;
  font-size: 40rpx;
}
.info .name{
  width: 100%;
  height: 60rpx;
}
.info .studentid{
  width: 100%;
  height: 60rpx;
}
.change{
  width: 100%;
  height: 85rpx;
  display: flex;
  justify-content: center;
  margin-top: 40rpx;
}
.change image{
  margin: 13rpx 0 0 0;
  width: 65rpx;
  height: 65rpx;
}
.change text{
  color: #53bafe; 
  font-size: 40rpx;
  line-height: 90rpx;
  padding-left: 25rpx;
}